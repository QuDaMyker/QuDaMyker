
#include <iostream>
using namespace std;
void xoa (int a[], int &n, int k)
{
  for (int i = k; i < n; i++)
    {
      a[i] = a[i + 1];
    }
  --n;
}

int main ()
{

  for (int i = n - 1; i >= 0; i--)
    {
      for (int j = i - 1; j >= 0; j--)
	{
	  if (a[j] == a[i])
	    {
	      xoa (a, n, i);
	    }
	}
    }
}
